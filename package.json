{
  "name": "@voomap/monorepo",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "up": "taze major -wIr",
    "typecheck": "pnpm -r typecheck",
    "watch": "pnpm -r --parallel watch",
    "build": "pnpm -r build",
    "build:map": "pnpm --filter @voomap/map build",
    "build:core": "pnpm --filter @voomap/core build",
    "play": "nr play:map",
    "play:core": "nr -C playgrounds/vite-core dev",
    "play:map": "nr -C playgrounds/vite-map dev",
    "clean": "rimraf --glob dist types \"packages/*/dist\"",
    "lint": "eslint --cache .",
    "lint:fix": "nr lint --fix",
    "docs:dev": "vitepress dev docs --port 2222",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 8080",
    "release": "bumpp -r && esno scripts/publish.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@antfu/ni": "catalog:dev",
    "@googlemaps/js-api-loader": "catalog:build",
    "@types/google.maps": "catalog:types",
    "@types/node": "catalog:types",
    "@vitejs/plugin-vue": "catalog:build",
    "@vue.ts/complex-types": "catalog:build",
    "@vueuse/core": "catalog:build",
    "@vueuse/shared": "catalog:build",
    "bumpp": "catalog:dev",
    "eslint": "catalog:dev",
    "esno": "catalog:dev",
    "lint-staged": "catalog:dev",
    "pnpm": "catalog:dev",
    "rimraf": "catalog:dev",
    "simple-git-hooks": "catalog:dev",
    "taze": "catalog:dev",
    "tsdown": "catalog:build",
    "typescript": "catalog:dev",
    "unplugin-vue": "catalog:build",
    "vite": "catalog:build",
    "vitepress": "catalog:build",
    "vue": "catalog:build",
    "vue-component-type-helpers": "catalog:build",
    "vue-tsc": "catalog:dev"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
