{
  "name": "@voomap/monorepo",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.3.0",
  "scripts": {
    "up": "taze major -wIr",
    "dev": "pnpm --filter './packages/**' dev",
    "build": "pnpm --filter './packages/**' build",
    "build:map": "pnpm --filter @voomap/map build",
    "build:core": "pnpm --filter @voomap/core build",
    "play": "nr play:map",
    "play:core": "nr -C playgrounds/vite-core dev",
    "play:map": "nr -C playgrounds/vite-map dev",
    "clean": "rimraf --glob dist types \"packages/*/dist\"",
    "lint": "eslint --cache .",
    "lint:fix": "nr lint --fix",
    "docs:dev": "vitepress dev docs --port 2222",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 8080",
    "release": "bumpp -r && esno scripts/publish.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@antfu/ni": "catalog:",
    "@googlemaps/js-api-loader": "catalog:",
    "@types/fs-extra": "catalog:",
    "@types/google.maps": "catalog:",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue.ts/complex-types": "catalog:",
    "@vue/tsconfig": "catalog:",
    "@vueuse/core": "catalog:",
    "@vueuse/shared": "catalog:",
    "bumpp": "catalog:",
    "consola": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-format": "catalog:",
    "esno": "catalog:",
    "fs-extra": "catalog:",
    "lint-staged": "catalog:",
    "pnpm": "catalog:",
    "rimraf": "catalog:",
    "simple-git-hooks": "catalog:",
    "taze": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vite-plugin-replace": "catalog:",
    "vite-plugin-style-inject": "catalog:",
    "vitepress": "catalog:",
    "vue": "catalog:",
    "vue-component-type-helpers": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
