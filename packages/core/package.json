{
  "name": "@voomap/core",
  "version": "0.0.1",
  "description": "Google Maps with composables",
  "author": "ilyaliao",
  "license": "MIT",
  "funding": "https://github.com/ilyaliao",
  "homepage": "https://github.com/ilyaliao/voomap#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ilyaliao/voomap"
  },
  "keywords": [
    "voomap",
    "vue3",
    "composition-api",
    "map",
    "composable",
    "google-map"
  ],
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "source": "src/index.ts",
  "scripts": {
    "typecheck": "vue-tsc --noEmit",
    "build": "unbuild",
    "dev": "unbuild --stub"
  },
  "peerDependencies": {
    "vue": "catalog:build"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "catalog:build",
    "@voomap/shared": "workspace:*",
    "@vueuse/core": "catalog:build",
    "@vueuse/shared": "catalog:build"
  },
  "devDependencies": {
    "@types/google.maps": "catalog:types"
  }
}
